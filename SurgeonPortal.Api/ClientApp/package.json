{
  "name": "surgeon-portal",
  "version": "0.0.0",
  "buildId": "Dev-1",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5001 --ssl",
    "local:start": "ng serve --ssl --port 5200 --proxy-config scripts/proxy.confg.json",
    "build": "ng build",
    "build-config": "node scripts/build-config.js",
    "watch": "ng build --watch --configuration development",
    "run-app": "cd ../ && dotnet run",
    "restore": "cd ../ && dotnet restore",
    "lib-build": "ng build ytg-angular",
    "lib-dev-build": "npm run lib-build && npm run ytg-angular:pack && node scripts/build-install-lib.js",
    "ytg-angular:pack": "cd dist/ytg-angular && npm pack .",
    "test-lint": "ng test --source-map=false --browsers=ChromeHeadless --watch=false",
    "test": "ng test",
    "lint": "eslint src --ext js,ts,tsx",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --format json",
    "i18n:write-to-excel": "node scripts/language/read-from-locales.js ./language-test i18n/en-US.json,i18n/comments.json",
    "i18n:read-from-excel": "node scripts/language/write-to-locales.js ./language-test/translations.xlsx ./i18n/",
    "i18n:generate-interface": "node scripts/language/generate-i18n-interface.js ./i18n/ en-US.json ./language-test/target-directories ./language-test/localization-directories",
    "generate-language": "npm run i18n:read-from-excel && npm run i18n:generate-interface",
    "prepare": "cd ../../ && husky install SurgeonPortal.Api/ClientApp/.husky",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "storybook": "ng run SurgeonPortal:storybook",
    "build-storybook": "ng run SurgeonPortal:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.1",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "15.2.1",
    "@angular/compiler": "15.2.1",
    "@angular/core": "15.2.1",
    "@angular/forms": "15.2.1",
    "@angular/platform-browser": "15.2.1",
    "@angular/platform-browser-dynamic": "15.2.1",
    "@angular/router": "15.2.1",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fullcalendar/angular": "^6.1.6",
    "@fullcalendar/core": "^6.1.6",
    "@fullcalendar/multimonth": "^6.1.6",
    "@ngneat/until-destroy": "^10.0.0-beta.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@ngxs/form-plugin": "^3.8.1",
    "@ngxs/router-plugin": "^3.8.1",
    "@ngxs/storage-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "@shoelace-style/shoelace": "^2.3.0",
    "chart.js": "^4.2.1",
    "esbuild": "0.18.11",
    "lit": "^2.7.4",
    "ng2-charts": "^4.1.1",
    "ngx-mask": "^15.1.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ytg-angular": "^15.20230609.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "15.2.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@compodoc/compodoc": "^1.1.21",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@ngxs/logger-plugin": "^3.8.1",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-interactions": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/angular": "^7.0.17",
    "@storybook/blocks": "^7.0.17",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.17",
    "typescript": "~4.9.4",
    "xlsx": "file:vendor/xlsx-0.19.3.tar"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
