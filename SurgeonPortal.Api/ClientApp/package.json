{
  "name": "surgeon-portal",
  "version": "0.0.0",
  "eslintIgnore": [
    "projects/ytg-n/src/lib/legacy"
  ],
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5200 --proxy-config proxy.confg.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "run-app": "cd ../ && dotnet run",
    "lib-build": "ng build ytg-n && ng build ytg-angular",
    "lib-dev-build": "npm run lib-build && npm run ytg-n:pack && npm run ytg-angular:pack && node scripts/build-install-lib.js",
    "ytg-n:pack": "cd dist/ytg-n && npm pack .",
    "ytg-angular:pack": "cd dist/ytg-angular && npm pack .",
    "lib-watch": "ng build ytg-n --watch",
    "test-lint": "ng test --source-map=false --browsers=ChromeHeadless --watch=false",
    "test": "ng test",
    "lint": "eslint src --ext js,ts,tsx",
    "prepare": "cd ../../ && husky install SurgeonPortal.Api/ClientApp/.husky",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "mockserver-start": "json-server --watch mock-server/db.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.1",
    "@angular/common": "15.2.1",
    "@angular/compiler": "15.2.1",
    "@angular/core": "15.2.1",
    "@angular/forms": "15.2.1",
    "@angular/platform-browser": "15.2.1",
    "@angular/platform-browser-dynamic": "15.2.1",
    "@angular/router": "15.2.1",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@ngxs/form-plugin": "^3.7.6",
    "@ngxs/router-plugin": "^3.7.6",
    "@ngxs/storage-plugin": "^3.7.6",
    "@ngxs/store": "^3.7.6",
    "@shoelace-style/shoelace": "^2.3.0",
    "chart.js": "^4.2.1",
    "lit": "^2.6.1",
    "ng2-charts": "^4.1.1",
    "ngx-mask": "^15.1.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ytg-angular": "file:ytg-angular-0.0.1.tgz",
    "ytg-n": "file:ytg-n-0.0.1.tgz",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "15.2.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@ngxs/devtools-plugin": "^3.7.6",
    "@ngxs/logger-plugin": "^3.7.6",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "json-server": "^0.17.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
